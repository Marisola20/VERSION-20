        android:textColor="#FFFFFF"
        android:textColorLink="#DDD5D5"
        android:textSize="26sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.735" />

    <Button
        android:id="@+id/btnDatosqr"
        android:layout_width="241dp"
        android:layout_height="81dp"
        android:backgroundTint="#363E68"
        android:onClick="irDatosQRConductor"
        android:text="Datos QR"
        android:textColor="#FFFFFF"
        android:textColorLink="#DDD5D5"
        android:textSize="26sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.618" />

    <Button
        android:id="@+id/btnSalirc"
        android:layout_width="156dp"
        android:layout_height="53dp"
        android:backgroundTint="#A34747"
        android:onClick="irCerrarSesion"
        android:text="Cerrar SesiÃ³n"
        android:textColor="#FFFFFF"
        android:textColorLink="#DDD5D5"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.946" />

    <TextView
        android:id="@+id/textView28"
        android:layout_width="203dp"
        android:layout_height="44dp"
        android:background="#EFE9E9"
        android:backgroundTint="#EFE9E9"
        android:gravity="center|right"
        android:text="Pago Confirmado"
        android:textColor="#363E68"
        android:textColorLink="#363E68"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.803" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
